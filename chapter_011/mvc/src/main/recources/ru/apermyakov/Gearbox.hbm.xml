<hibernate-mapping>
    <class name="ru.apermyakov.model.car.Gearbox" table="gearbox">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="name" column="name"/>
        <property name="type" column="type"/>
        <property name="gear" column="gear"/>
    </class>
</hibernate-mapping>